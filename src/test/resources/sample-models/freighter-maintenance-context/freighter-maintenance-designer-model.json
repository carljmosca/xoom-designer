{
  "context": {
    "groupId": "io.vlingo.cargoshipping",
    "artifactId": "freighter-maintenance",
    "artifactVersion": "1.0.0",
    "packageName": "io.vlingo.cargoshipping.freightermaintenance"
  },
  "model": {
    "persistenceSettings": {
      "storageType": "STATE_STORE",
      "useCQRS": true,
      "projections": "EVENT_BASED",
      "database": "IN_MEMORY",
      "commandModelDatabase": "IN_MEMORY",
      "queryModelDatabase": "IN_MEMORY"
    },
    "aggregateSettings": [
      {
        "api": {
          "rootPath": "/maintenanceOrder",
          "routes": [
            {
              "path": "*",
              "httpMethod": "POST",
              "aggregateMethod": "schedule",
              "requireEntityLoad": false
            }
          ]
        },
        "aggregateName": "MaintenanceOrder",
        "stateFields": [
          {
            "name": "id",
            "type": "String"
          },
          {
            "name": "freighterId",
            "type": "String",
            "collectionType": ""
          },
          {
            "name": "freighterPartNumber",
            "type": "String",
            "collectionType": ""
          }
        ],
        "methods": [
          {
            "name": "schedule",
            "parameters": [
              {
                "stateField": "freighterId",
                "parameterName": "freighterId",
                "multiplicity": ""
              },
              {
                "stateField": "freighterPartNumber",
                "parameterName": "freighterPartNumber",
                "multiplicity": ""
              }
            ],
            "useFactory": true,
            "event": "MaintenanceOrderScheduled"
          }
        ],
        "events": [
          {
            "name": "MaintenanceOrderScheduled",
            "fields": [
              "id",
              "freighterId",
              "freighterPartNumber"
            ]
          }
        ],
        "consumerExchange": {
          "exchangeName": "",
          "receivers": [
          ]
        },
        "producerExchange": {
          "exchangeName": "",
          "schemaGroup": "",
          "outgoingEvents": []
        }
      }
    ],
    "valueObjectSettings": []
  },
  "deployment": {
    "type": "NONE",
    "dockerImage": "",
    "kubernetesImage": "",
    "kubernetesPod": "",
    "clusterTotalNodes": 3,
    "clusterPort": 50011,
    "producerExchangePort": 19762,
    "httpServerPort": 8081,
    "pullSchemas": false
  },
  "schemata": {
    "host": "localhost",
    "port": 9019
  },
  "projectDirectory": "%s",
  "useAnnotations": true,
  "useAutoDispatch": true,
  "generateUIWith": "ReactJS",
  "generateUI": false
}