                "stateField": "candidateDoers",
                "parameterName": "candidateDoers",
                "multiplicity": ""
              }
            ],
            "useFactory": false,
            "event": "CandidateDoersRecorded"
          },
          {
            "name": "mergeCandidateDoers",
            "parameters": [
              {
                "stateField": "candidateDoers",
                "parameterName": "candidateDoers",
                "multiplicity": ""
              }
            ],
            "useFactory": false,
            "event": "CandidateDoersLocated"
